/**index.wxss**/
.container {
  margin: 0 16rpx;
  text-align: start;
  padding: 0 0 150rpx;
}

.h2 {
  display: block;
  font-size: 1.5em;
  margin-block-start: 0.83em;
  margin-block-end: 0.83em;
  margin-inline-start: 0px;
  margin-inline-end: 0px;
  font-weight: bold;
  width: 100%;
}

.h3 {
  display: block;
  font-size: 1.17em;
  margin-block-start: 1em;
  margin-block-end: 1em;
  margin-inline-start: 0px;
  margin-inline-end: 0px;
  font-weight: bold;
  width: 100%;
}

.link-box {
  display: flex;
  justify-content: flex-end;
  margin: 30rpx 0;
}

view {
  word-break: break-all;
}

.link {
  color: #1054e7;
  text-align: end;
}